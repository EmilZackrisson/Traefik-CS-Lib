<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <RootNamespace>Traefik</RootNamespace>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Version>0.0.2.1</Version>
        <Authors>EmilZackrisson</Authors>
        <Description>A library for reading, adding and editing a YAML Traefik dynamic config.</Description>
        <Copyright>Emil Zackrisson</Copyright>
        <PackageReleaseNotes>Add Error when config file not exists.</PackageReleaseNotes>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="NLog" Version="5.2.7" />
      <PackageReference Include="YamlDotNet" Version="13.7.1" />
    </ItemGroup>

</Project>
